def sum_of_divisors(n):
    if n == 1:
        return 1
    total = 1 + n  # include 1 and n
    for d in range(2, int(n**0.5) + 1):
        if n % d == 0:
            total += d
            other = n // d
            if other != d:
                total += other
    return total

def S(N):
    S = 0
    for i in range(1,N+1):
        for j in range(1,N+1):
            S = S + sum_of_divisors(i*j)
            
    return S
        
print(S(1000)%(10**9))